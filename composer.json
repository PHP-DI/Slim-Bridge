{
    "name": "php-di/slim-bridge",
    "description": "PHP-DI integration in Slim",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "DI\\Bridge\\Slim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DI\\Bridge\\Slim\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "~5.6|~7.0",
        "php-di/php-di": "^5.2",
        "php-di/kernel": "~0.1.1",
        "php-di/invoker": "^1.2",
        "slim/slim": "^3.0",
        "mnapoli/silly-php-di": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.1"
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
